          <enabled>true</enabled>
        </config>
      </subinterface>
      <!-- eth5.20 -->
      <subinterface>
        <index>20</index>
        <config>
          <index>20</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>30.10.2.1</ip>
              <config>
                <ip>30.10.2.1</ip>
                <prefix-length>24</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
    </subinterfaces>
  </interface>
</interfaces>

<interfaces xmlns="https://github.com/beluganos/beluganos/yang/interfaces">
  <!--
      +- interface(eth1)
      |  +- subinterface(eth1)
      |  +- subinterface(eth1.100)
      |  |  +- 10.10.1.6/30
      |  +- subinterface(eth1.200)
      |     +- 20.10.1.6/24
      +- interface(eth2)
      |  +- subinterface(eth2)
      |  +- subinterface(eth2.100)
      |  |  +- 10.10.2.6/30
      |  +- subinterface(eth2.200)
      |     +- 20.10.2.6/24
  -->
  <interface>
    <name>eth1</name>
    <config>
      <name>eth1</name>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
    </config>
    <subinterfaces>
      <!-- eth1 -->
      <subinterface>
        <index>0</index>
        <config>
          <index>0</index>
          <enabled>true</enabled>
        </config>
      </subinterface>
      <!-- eth1.100 -->
      <subinterface>
        <index>100</index>
        <config>
          <index>100</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>10.10.1.6</ip>
              <config>
                <ip>10.10.1.6</ip>
                <prefix-length>30</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
      <!-- eth1.200 -->
      <subinterface>
        <index>200</index>
        <config>
          <index>200</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>20.10.1.6</ip>
              <config>
                <ip>20.10.1.6</ip>
                <prefix-length>24</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
    </subinterfaces>
  </interface>
  <interface>
    <name>eth2</name>
    <config>
      <name>eth2</name>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
    </config>
    <subinterfaces>
      <!-- eth2 -->
      <subinterface>
        <index>0</index>
        <config>
          <index>0</index>
          <enabled>true</enabled>
        </config>
      </subinterface>
      <!-- eth2.100 -->
      <subinterface>
        <index>100</index>
        <config>
          <index>100</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>10.10.2.6</ip>
              <config>
                <ip>10.10.2.6</ip>
                <prefix-length>30</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
    </subinterfaces>
  </interface>
  <interface>
    <name>eth3</name>
    <config>
      <name>eth3</name>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
    </config>
    <subinterfaces>
      <!-- eth3 -->
      <subinterface>
        <index>0</index>
        <config>
          <index>0</index>
          <enabled>true</enabled>
        </config>
      </subinterface>
      <!-- eth3.10 -->
      <subinterface>
        <index>10</index>
        <config>
          <index>10</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>20.10.1.1</ip>
              <config>
                <ip>20.10.1.1</ip>
                <prefix-length>24</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
    </subinterfaces>
  </interface>
  <interface>
    <name>eth4</name>
    <config>
      <name>eth4</name>
      <type xmlns:ianaift="urn:ietf:params:xml:ns:yang:iana-if-type">ianaift:ethernetCsmacd</type>
    </config>
    <subinterfaces>
      <!-- eth4 -->
      <subinterface>
        <index>0</index>
        <config>
          <index>0</index>
          <enabled>true</enabled>
        </config>
      </subinterface>
      <!-- eth4.20 -->
      <subinterface>
        <index>20</index>
        <config>
          <index>20</index>
          <enabled>true</enabled>
        </config>
        <ipv4 xmlns="https://github.com/beluganos/beluganos/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>30.10.1.1</ip>
              <config>
                <ip>30.10.1.1</ip>
                <prefix-length>24</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </subinterface>
    </subinterfaces>
  </interface>
  <interface>
    <name>eth5</name>
    <config>

<network-instances xmlns="https://github.com/beluganos/beluganos/yang/network-instance">
  <network-instance>
    <name>R1</name>
    <config>
      <name>R1</name>
      <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">oc-ni-types:DEFAULT_INSTANCE</type>
    </config>
    <loopbacks/>
    <interfaces/>
    <protocols>
      <protocol>
        <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:OSPF</identifier>
        <name>ospfv2</name>
        <config>
          <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:OSPF</identifier>
          <name>ospfv2</name>
        </config>
        <ospfv2>
          <global>
            <config>
              <router-id>10.0.0.1</router-id>
            </config>
          </global>
          <areas>
            <area>
              <identifier>0.0.0.0</identifier>
              <config>
                <identifier>0.0.0.0</identifier>
              </config>
              <interfaces>
                <interface>
                  <id>eth1</id>
                  <config>
                    <id>eth1</id>
                    <metric>100</metric>
                    <passive>false</passive>
                  </config>
                  <interface-ref>
                    <config>
                      <interface>eth1</interface>
                      <subinterface>0</subinterface>
                    </config>
                  </interface-ref>
                  <timers>
                    <config>
                      <hello-interval>40</hello-interval>
                      <dead-interval>10</dead-interval>
                    </config>
                  </timers>
                </interface>
                <interface>
                  <id>lo</id>
                  <config>
                    <id>lo</id>
                    <metric>10</metric>
                    <passive>true</passive>
                  </config>
                  <interface-ref>
                    <config>
                      <interface>lo</interface>
                      <subinterface>0</subinterface>
                    </config>
                  </interface-ref>
                  <timers>
                    <config/>
                  </timers>
                </interface>
              </interfaces>
              <ranges/>
            </area>
          </areas>
        </ospfv2>
      </protocol>
      <protocol>
        <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:OSPF6</identifier>
        <name>ospfv3</name>
        <config>
          <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:OSPF6</identifier>
          <name>ospfv3</name>
        </config>
        <ospfv3>
          <global>
            <config>
              <router-id>10.0.0.1</router-id>
            </config>
          </global>
          <areas>
            <area>
              <identifier>0.0.0.0</identifier>
              <config>
                <identifier>0.0.0.0</identifier>
              </config>
              <interfaces>
                <interface>
                  <id>eth1</id>
                  <config>
                    <id>eth1</id>
                    <metric>100</metric>
                    <passive>false</passive>
                  </config>
                  <interface-ref>
                    <config>
                      <interface>eth1</interface>
                      <subinterface>0</subinterface>
                    </config>
                  </interface-ref>
                  <timers>
                    <config>
                      <hello-interval>40</hello-interval>
                      <dead-interval>10</dead-interval>
                    </config>
                  </timers>
                </interface>
                <interface>
                  <id>lo</id>
                  <config>
                    <id>lo</id>
                    <metric>10</metric>
                    <passive>true</passive>
                  </config>
                  <interface-ref>
                    <config>
                      <interface>lo</interface>
                      <subinterface>0</subinterface>
                    </config>
                  </interface-ref>
                  <timers>
                    <config/>
                  </timers>
                </interface>
              </interfaces>
              <ranges>
                <range>
                  <ip>2001::1</ip>
                  <prefix-length>128</prefix-length>
                  <config>
                    <ip>2001::1</ip>
                    <prefix-length>128</prefix-length>
                  </config>
                </range>
                <range>
                  <ip>2001:db8:2::</ip>
                  <prefix-length>64</prefix-length>
                  <config>
                    <ip>2001:db8:2::</ip>
                    <prefix-length>64</prefix-length>
                  </config>
                </range>
              </ranges>
            </area>
          </areas>
        </ospfv3>
      </protocol>
      <protocol>
        <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:BGP</identifier>
        <name>BGP</name>
        <config>
          <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">oc-pol-types:BGP</identifier>
          <name>BGP</name>
        </config>
        <bgp>
          <global>
            <config>
              <as>65001</as>
              <router-id>10.0.0.1</router-id>
            </config>
          </global>
          <zebra>
            <config>
              <enabled>true</enabled>
              <version>5</version>
              <url>unix:/var/run/frr/zserv.api</url>
            </config>
          </zebra>
          <neighbors>
            <neighbor>
              <neighbor-address>10.0.0.4</neighbor-address>
              <config>
                <neighbor-address>10.0.0.4</neighbor-address>
                <peer-as>65001</peer-as>
              </config>
              <timers>
                <config>
                  <hold-time>3600</hold-time>
                  <keepalive-interval>180</keepalive-interval>
                </config>
              </timers>
              <transport>
                <config>
                  <local-address>10.0.0.1</local-address>
                </config>
              </transport>
              <afi-safis>
                <afi-safi>
                  <afi-safi-name xmlns:oc-bgp-types="http://openconfig.net/yang/bgp-types">oc-bgp-types:IPV4_UNICAST</afi-safi-name>
                  <config>
                    <afi-safi-name xmlns:oc-bgp-types="http://openconfig.net/yang/bgp-types">oc-bgp-types:IPV4_UNICAST</afi-safi-name>
                  </config>
                </afi-safi>
              </afi-safis>
              <apply-policy>
                <config>
                  <default-import-policy>ACCEPT_ROUTE</default-import-policy>
                  <default-export-policy>ACCEPT_ROUTE</default-export-policy>
                  <export-policy>policy-next-hop-self</export-policy>
                </config>
              </apply-policy>
            </neighbor>
            <neighbor>
              <neighbor-address>2001:db8:3::2</neighbor-address>
              <config>
                <neighbor-address>2001:db8:3::2</neighbor-address>
                <peer-as>65001</peer-as>
              </config>
              <timers>
                <config>
                  <hold-time>3600</hold-time>
                  <keepalive-interval>180</keepalive-interval>
                </config>
              </timers>
              <transport>
                <config/>
              </transport>
              <afi-safis>
                <afi-safi>
                  <afi-safi-name xmlns:oc-bgp-types="http://openconfig.net/yang/bgp-types">oc-bgp-types:IPV6_UNICAST</afi-safi-name>
                  <config>
                    <afi-safi-name xmlns:oc-bgp-types="http://openconfig.net/yang/bgp-types">oc-bgp-types:IPV6_UNICAST</afi-safi-name>
                  </config>
                </afi-safi>
              </afi-safis>
              <apply-policy>
                <config>
                  <default-import-policy>ACCEPT_ROUTE</default-import-policy>
                  <default-export-policy>ACCEPT_ROUTE</default-export-policy>
                  <export-policy>policy-next-hop-self</export-policy>
                </config>
              </apply-policy>
            </neighbor>
          </neighbors>
        </bgp>
      </protocol>
    </protocols>
  </network-instance>
</network-instances>

